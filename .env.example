# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Treasury Wallet Address (Solana)
VITE_TREASURY_WALLET_ADDRESS=your_treasury_wallet_address_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anonymous_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Master Admin Key for Admin Panel
VITE_MASTER_ADMIN_KEY=your_master_admin_key_here

# Encryption Key for Wallet Private Keys
VITE_WALLET_ENCRYPTION_KEY=your_strong_encryption_key_here

# Optional: Gemini API Key (if used)
GEMINI_API_KEY=your_gemini_api_key_here

# Development/Debug Settings
DEBUG_TRANSACTIONS=true
SOLANA_CLUSTER=devnet
